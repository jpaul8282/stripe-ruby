on:
  push:
    branches:
    - main
    - release/*
on:
  pull_request:
    branches:
    - main
on:
  schedule:
  - cron: "0 2 * * 1-5"
on:
  workflow_dispatch:
jobs:
  my_job:
    name: deploy to staging
    runs-on: ubuntu-22.04
- name: Setup Node
  uses: actions/setup-node@v4
  with:
    node-version: '20.x'
- name: Install Dependencies
  run: npm install
